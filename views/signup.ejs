  <!-- Cart view section -->
  <section id="aa-myaccount">
    <div class="container">
      <div class="row">
        <div class="col-sm-6">
          <div class="aa-myaccount-area">
            <div class="aa-myaccount-register">
              <h4>User Register</h4>
              <form method="POST" action="/signup" class="aa-login-form" class="form-group">
                <label for="">Name<span>*</span></label>
                <input type="text" placeholder="name" name="name" class="form-control">
                <label for="">Email address<span>*</span></label>
                <input type="email" placeholder="email" name="email" class="form-control">
                <label for="">Phone Number<span>*</span></label>
                <input type="tel" placeholder="phone no" name="phone" class="form-control">
                <label for="">Username<span>*</span></label>
                <input type="text" placeholder="username" name="username" class="form-control">
                <label for="">Password<span>*</span></label>
                <input type="password" placeholder="Password" name="password" class="form-control">
                <label for="">Confirm Password<span>*</span></label>
                <input type="password" placeholder="Password" class="form-control">
                <button type="submit" class="aa-browse-btn" onclick="alert('Registered Successfully');">Register</button>
              </form>
            </div>
          </div>
        </div>
        <div class="col-sm-6">
          <div class="aa-myaccount-area">
            <div class="aa-myaccount-register">
              <h4>Shop Register</h4>
              <form method="POST" action="/adminRegister" class="aa-login-form" class="form-group">
                <label for="">Name<span>*</span></label>
                <input type="text" placeholder="name" name="name" class="form-control">
                <label for="">Shop Name:<span>*</span></label>
                <input type="text" placeholder="shop name" name="shopname" class="form-control">
                <label for="">Email address<span>*</span></label>
                <input type="email" placeholder="email" name="email" class="form-control">
                <label for="">Phone Number<span>*</span></label>
                <input type="tel" placeholder="phone no" name="phone" class="form-control">
                <label for="">Address<span>*</span> <a href=""><span class="glyphicon glyphicon-map-marker"></span></a></label>
                <textarea class="form-control" rows="3" placeholder="address" name="address"></textarea>
                <label for="">Area<span>*</span></label>
                <input type="text" placeholder="Area" name="area" class="form-control">
                <label for="">Username<span>*</span></label>
                <input type="text" placeholder="username" name="username" class="form-control">
                <label for="">Password<span>*</span></label>
                <input type="password" placeholder="Password" name="password" class="form-control">
                <label for="">Confirm Password<span>*</span></label>
                <input type="password" placeholder="Password" class="form-control">
                <button type="submit" class="aa-browse-btn" onclick="alert('Registered Successfully');">Register</button>
              </form>
            </div>
          </div>
        </div>
      </div>
